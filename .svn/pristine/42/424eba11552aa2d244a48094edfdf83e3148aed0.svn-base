.edit-table{
	width: 100%;
	border:solid #d4d4d4; 
	border-width:1px 0px 0px 1px;
	border-collapse:separate;
    border-spacing: 0px;
    
}
.edit-table td{
	border:solid #d4d4d4; 
	border-width:0px 1px 1px 0px;
	padding: 5px;
}

.edit-table .td-label-1{
	width: 30%;
	text-align: right;
	background-color: #f8fbfc;
	font-weight: bold;
}
.edit-table .td-input-1{
	width: 70%;
}